{
  "version": "v1",
  "metadata": {
    "name": "speculative-vad-debug",
    "description": "Speculative VAD debug: low-latency forwarding with cancellation messages (no TTS)"
  },
  "nodes": [
    {
      "id": "audio_resampler",
      "node_type": "FastResampleNode",
      "params": {
        "sourceRate": 48000,
        "targetRate": 16000,
        "quality": "Low",
        "channels": 1
      },
      "is_streaming": true
    },
    {
      "id": "vad_chunker",
      "node_type": "AudioChunkerNode",
      "params": {
        "chunkSize": 320
      },
      "is_streaming": true
    },
    {
      "id": "speculative_vad",
      "node_type": "SpeculativeVADGate",
      "params": {
        "lookbackMs": 100,
        "lookaheadMs": 25,
        "vadThreshold": 0.5,
        "sampleRate": 16000,
        "minSpeechMs": 200,
        "minSilenceMs": 300,
        "padMs": 12
      },
      "is_streaming": true
    }
  ],
  "connections": [
    { "from": "audio_resampler", "to": "vad_chunker" },
    { "from": "vad_chunker", "to": "speculative_vad" }
  ]
}
