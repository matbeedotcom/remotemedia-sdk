{
  "version": "v1",
  "metadata": {
    "name": "test-multiprocess-pipeline",
    "description": "Test pipeline with Python multiprocess nodes for spec 002",
    "multiprocess": {
      "max_processes_per_session": 5,
      "channel_capacity": 100,
      "init_timeout_secs": 30,
      "python_executable": "python",
      "enable_backpressure": true
    }
  },
  "nodes": [
    {
      "id": "input_chunker",
      "node_type": "AudioChunkerNode",
      "params": {
        "chunk_size": 1024
      }
    },
    {
      "id": "whisper_node",
      "node_type": "WhisperNode",
      "params": {
        "model": "base",
        "language": "en"
      }
    },
    {
      "id": "lfm2_node",
      "node_type": "LFM2Node",
      "params": {
        "model": "small"
      }
    }
  ],
  "connections": [
    {
      "from": "input_chunker",
      "to": "whisper_node"
    },
    {
      "from": "whisper_node",
      "to": "lfm2_node"
    }
  ]
}
