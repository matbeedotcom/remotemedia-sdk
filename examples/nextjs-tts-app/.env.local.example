# Local Development Configuration
# Copy this file to .env.local and adjust values as needed

# gRPC Service Endpoint (Server-side, used by API routes)
GRPC_HOST=localhost:50051

# Feature Flags
NEXT_PUBLIC_ENABLE_VOICE_SELECTION=true
NEXT_PUBLIC_ENABLE_SPEED_CONTROL=true
NEXT_PUBLIC_MAX_TEXT_LENGTH=10000

# Development Mode
NEXT_PUBLIC_DEBUG_MODE=true
