{
  "name": "nextjs-tts-app",
  "version": "0.1.0",
  "private": true,
  "description": "Real-time text-to-speech web application using Next.js and RemoteMedia gRPC service",
  "scripts": {
    "dev": "next dev",
    "dev:server": "tsx server.ts",
    "build": "next build",
    "start": "next start",
    "server": "tsx server.ts",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "generate:protos": "protoc --ts_out src/generated --ts_opt generate_dependencies --proto_path ../../runtime/protos ../../runtime/protos/*.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.0",
    "@grpc/proto-loader": "^0.7.13",
    "@protobuf-ts/grpcweb-transport": "^2.11.1",
    "@protobuf-ts/runtime": "^2.11.1",
    "@protobuf-ts/runtime-rpc": "^2.11.1",
    "next": "^16.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "server-only": "^0.0.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^9.0.1",
    "ws": "^8.18.3",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.11.1",
    "@types/node": "^20.11.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.38.0",
    "eslint-config-next": "^16.0.1",
    "postcss": "^8.4.0",
    "prettier": "^3.2.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.20.6",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18.17.0",
    "pnpm": ">=8.0.0"
  }
}
