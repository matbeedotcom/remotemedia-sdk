{
  "version": "v1",
  "metadata": {
    "name": "simple-docker-node-example",
    "description": "Docker-based Python node with iceoryx2 IPC - SimplePyTorchNode for testing"
  },
  "nodes": [
    {
      "id": "pytorch_docker",
      "node_type": "SimplePyTorchNode",
      "is_streaming": true,
      "params": {
        "operation": "echo"
      },
      "docker": {
        "python_version": "3.11",
        "python_packages": [
          "iceoryx2",
          "torch>=2.0.0"
        ],
        "resource_limits": {
          "memory_mb": 2048,
          "cpu_cores": 1.0,
          "gpu_devices": ["all"]
        }
      }
    }
  ],
  "connections": []
}
