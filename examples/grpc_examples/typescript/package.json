{
  "name": "@remotemedia/typescript-examples",
  "version": "0.1.0",
  "private": true,
  "description": "TypeScript examples for RemoteMedia gRPC service",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "simple": "node dist/simple_execution.js",
    "multi-node": "node dist/multi_node_pipeline.js",
    "streaming": "node dist/streaming_example.js",
    "audio": "node dist/audio_pipeline.js",
    "streaming-audio": "node dist/streaming_audio_pipeline.js",
    "all": "npm run simple && npm run multi-node && npm run streaming && npm run audio && npm run streaming-audio",
    "dev:simple": "tsx simple_execution.ts",
    "dev:multi-node": "tsx multi_node_pipeline.ts",
    "dev:streaming": "tsx streaming_example.ts",
    "dev:audio": "tsx audio_pipeline.ts",
    "dev:streaming-audio": "tsx streaming_audio_pipeline.ts"
  },
  "dependencies": {
    "@remotemedia/nodejs-client": "file:../../../nodejs-client"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
